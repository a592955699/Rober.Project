{
  "Nets": [
    {
      "Name": "WebApiProxy",
      "Uri": "http://localhost:8201/ActionExcutor.ashx",
      "Porxy": "Rober.Framework.Net.Client.WebApiProxy"
    },
    {
      "Name": "SoapProxy",
      "Uri": "http://localhost:8201/SoapService.svc",
      "Porxy": "Rober.Core.Http.Proxy.SoapProxy"
    }
  ],
  "Interceptors": [
    //{
    //  "Type": "Rober.Core.Action.Interceptors.LogInterceptor,Rober.Core",
    //  "IncluedCommands": [],
    //  "ExcludeCommands": []
    //},
    {
      "Type": "Rober.Core.Action.Interceptors.SessionAuthorityInterceptor,Rober.Core",
      "IncluedCommands": [],
      "ExcludeCommands": [ "LoginAction" ]
    }
  ],
  "Hosting": {
    "UseHttpClusterHttps": false,
    "UseHttpXForwardedProto": false,
    "ForwardedHttpHeader": ""
  },
  "ConnectionStrings": {
    "JmObjectContext": "Data Source=.;Initial Catalog=JmProject;Integrated Security=False;Persist Security Info=False;User ID=sa;Password=123"
  },
  "Jm": {
    "DisplayFullErrorStack": false,
    "StaticFilesCacheControl": "public,max-age=604800",
    "UseResponseCompression": false,
    "AzureBlobStorageConnectionString": "",
    "AzureBlobStorageContainerName": "",
    "AzureBlobStorageEndPoint": "",
    "RedisCachingEnabled": true,
    "RedisCachingConnectionString": "127.0.0.1:6379,ssl=False",
    "PersistDataProtectionKeysToRedis": false,
    "UserAgentStringsPath": "~/App_Data/browscap.xml",
    "CrawlerOnlyUserAgentStringsPath": "~/App_Data/browscap.crawlersonly.xml",
    "SupportPreviousNopcommerceVersions": true,
    "DisableSampleDataDuringInstallation": false,
    "UseFastInstallationService": false,
    "PluginsIgnoredDuringInstallation": "",
    "IgnoreStartupTasks": false,
    "ClearPluginShadowDirectoryOnStartup": true,
    "UseUnsafeLoadAssembly": true
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    },
    "LogLevel": {
      "Default": "Debug"
    }
  },
  "AllowedHosts": "*"
}
